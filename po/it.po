#: ../usr/bin/updatemanager:18 ../usr/bin/updatemanager:91
#: ../usr/lib/solydxk/updatemanager/updatemanagertray.py:49
#: ../usr/share/applications/updatemanager.desktop.in.h:1
msgid "Update Manager"
msgstr "Gestore aggiornamenti"

#: ../usr/bin/updatemanager:46
#: ../usr/lib/solydxk/updatemanager/umglobal.py:388
msgid "Please enter your password"
msgstr "Per favore, digitare la password per avviare il gestore aggiornamenti"

#: ../usr/bin/updatemanager:89
msgid "Quick update silently updates your system."
msgstr "Aggiornamento rapido aggiorna silenziosamente il tuo sistema."

#: ../usr/bin/updatemanager:90
msgid "Quick update"
msgstr "Aggiornamento rapido"

#: ../usr/bin/updatemanager:110
msgid ""
"Update Manager cannot be started in a live environment\n"
"You can use the --force argument to start UM in a live environment"
msgstr ""
"Update Manager non può essere eseguito in un live environment\n"
"\n"
"Per forzare l'avvio, usare l'argomento --force"

#: ../usr/lib/solydxk/updatemanager/umrefresh.py:35
#: ../usr/lib/solydxk/updatemanager/updatemanager.py:158
msgid "Your system is up to date"
msgstr "Il sistema è aggiornato"

#: ../usr/lib/solydxk/updatemanager/umrefresh.py:36
msgid "There are updates available"
msgstr "Aggiornamenti disponibili"

#: ../usr/lib/solydxk/updatemanager/umrefresh.py:37
msgid "No internet connection"
msgstr "Nessuna connessione internet"

#: ../usr/lib/solydxk/updatemanager/umrefresh.py:38
msgid "Unable to retrieve sources information"
msgstr "Impossibile scaricare informazioni per sources"

#: ../usr/lib/solydxk/updatemanager/umrefresh.py:43
msgid "Refreshing..."
msgstr "Scaricamento informazioni..."

#: ../usr/lib/solydxk/updatemanager/umrefresh.py:58
#, python-format
msgid "New update: %s"
msgstr "Nuovi aggiornamenti: %s"

#: ../usr/lib/solydxk/updatemanager/updatemanager.py:87
msgid "Apt error"
msgstr "Errore di APT"

#: ../usr/lib/solydxk/updatemanager/updatemanager.py:88
msgid "The following packages will be upgraded:"
msgstr "I seguenti pacchetti verranno aggiornati:"

#: ../usr/lib/solydxk/updatemanager/updatemanager.py:89
msgid "The following NEW packages will be installed:"
msgstr "I seguenti NUOVI pacchetti verranno installati:"

#: ../usr/lib/solydxk/updatemanager/updatemanager.py:90
msgid "The following packages will be REMOVED:"
msgstr "Il seguenti pacchetti verranno rimossi:"

#: ../usr/lib/solydxk/updatemanager/updatemanager.py:91
msgid "The following packages have been kept back:"
msgstr "I seguenti pacchetti verranno mantenuti alla versione attuale:"

#: ../usr/lib/solydxk/updatemanager/updatemanager.py:92
msgid "The following packages are going to be downgraded:"
msgstr "I seguenti pacchetti verranno portati ad una versione meno recente:"

#: ../usr/lib/solydxk/updatemanager/updatemanager.py:151
msgid "SolydXK Update Manager"
msgstr "Gestore degli aggiornamenti di SolydXK"

#: ../usr/lib/solydxk/updatemanager/updatemanager.py:152
msgid "Install"
msgstr "Installa"

#: ../usr/lib/solydxk/updatemanager/updatemanager.py:153
#: ../usr/lib/solydxk/updatemanager/updatemanagertray.py:46
msgid "Refresh"
msgstr "Scaricamento informazioni..."

