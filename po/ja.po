#: ../usr/lib/solydxk/updatemanager/umrefresh.py:52
msgid "Refreshing..."
msgstr "更新中..."

#: ../usr/lib/solydxk/updatemanager/umrefresh.py:62
#, python-format
msgid "New update: %s"
msgstr "新たなアップデート: %s"

#: ../usr/lib/solydxk/updatemanager/umrefresh.py:66
#: ../usr/lib/solydxk/updatemanager/umglobal.py:51
msgid "There are updates available"
msgstr "利用できるアップデートが存在します"

#: ../usr/lib/solydxk/updatemanager/umrefresh.py:79
msgid "Unable to retrieve sources information"
msgstr "ソース情報を見つける事ができません"

